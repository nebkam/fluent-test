{
  "name": "nebkam/fluent-test",
  "description": "A few helpers to ease functional API testing in Symfony",
  "license": "MIT",
  "require": {
    "php": "^7.2 || ^8.0",
    "symfony/framework-bundle": "^4.4.7 || ^5.0.7 || 6.* || 7.*",
    "symfony/http-foundation": "^4.4.7 || ^5.0.7 || 6.* || 7.*",
    "symfony/browser-kit": "^4.4.7 || ^5.0.7 || 6.* || 7.*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Nebkam\\FluentTest\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nebkam\\FluentTest\\Test\\": "tests"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^5.0 || 6.* || 7.*"
  }
}
